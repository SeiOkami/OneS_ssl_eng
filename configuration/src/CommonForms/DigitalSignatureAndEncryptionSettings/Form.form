  <attributes>
    <name>Certificates</name>
    <title>
      <key>en</key>
      <value>Certificates</value>
    </title>
    <id>8</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Certificates.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Certificates.Revoked</segments>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CertificatesOverridable.Ref AS Ref,
	CertificatesOverridable.DeletionMark AS DeletionMark,
	CertificatesOverridable.Description AS Description,
	CertificatesOverridable.IssuedTo AS IssuedTo,
	CertificatesOverridable.Firm AS Firm,
	CertificatesOverridable.LastName AS LastName,
	CertificatesOverridable.Name AS Name,
	CertificatesOverridable.MiddleName AS MiddleName,
	CertificatesOverridable.JobTitle AS JobTitle,
	CertificatesOverridable.IssuedBy AS IssuedBy,
	CertificatesOverridable.ValidBefore AS ValidBefore,
	CertificatesOverridable.Signing AS Signing,
	CertificatesOverridable.Encryption AS Encryption,
	CertificatesOverridable.Thumbprint AS Thumbprint,
	CertificatesOverridable.CertificateData AS CertificateData,
	CertificatesOverridable.Application AS Application,
	CertificatesOverridable.Revoked AS Revoked,
	CertificatesOverridable.EnterPasswordInDigitalSignatureApplication AS EnterPasswordInDigitalSignatureApplication,
	CertificatesOverridable.Organization AS Organization,
	CASE
		WHEN CertificatesOverridable.User = VALUE(Catalog.Users.EmptyRef)
			THEN &quot;                                                                                                   &quot;
		ELSE CertificatesOverridable.User
	END AS User,
	CertificatesOverridable.Added AS Added,
	&amp;AdditionalField AS RequestStatus,
	CertificatesOverridable.Predefined AS Predefined,
	CertificatesOverridable.PredefinedDataName AS PredefinedDataName,
	CASE
		WHEN CertificatesOverridable.DeletionMark
			THEN 1
		WHEN &amp;IsRequest
			THEN 5
		ELSE CASE
			WHEN CertificatesOverridable.Thumbprint IN (&amp;CertificatesInPersonalStorage)
				THEN CASE
					WHEN CertificatesOverridable.ValidBefore &lt; &amp;CurrentDate
						THEN 4
					WHEN CertificatesOverridable.ValidBefore &lt; DATEADD(&amp;CurrentDate, DAY, 30)
						THEN 3
					ELSE 2
				END
			ELSE 0
		END
	END AS CertificateStatusInPersonalStorage,
	CASE
		WHEN NOT CertificatesOverridable.DeletionMark
		AND NOT CertificatesOverridable.Revoked
		AND CertificatesOverridable.ValidBefore >= &amp;CurrentDate
		AND CertificatesOverridable.ValidBefore &lt;= DATEADD(&amp;CurrentDate, DAY, 30)
			THEN TRUE
		ELSE FALSE
	END AS DeadlineEndsSoon,
	CASE
		WHEN NOT CertificatesOverridable.Revoked
		AND CertificatesOverridable.ValidBefore > &amp;CurrentDate
			THEN TRUE
		ELSE FALSE
	END AS Valid_SSLyf,
	CASE
		WHEN ProgramsOverridable.IsBuiltInCryptoProvider
			THEN 4
		WHEN CertificatesOverridable.Application = UNDEFINED
		OR CertificatesOverridable.Application = VALUE(Catalog.DigitalSignatureAndEncryptionApplications.EmptyRef)
		OR NOT ProgramsOverridable.Ref IS NULL
			THEN 1
		ELSE 3
	END + CASE
		WHEN CertificatesOverridable.DeletionMark
			THEN 4
		ELSE 0
	END AS LocationType
FROM
	Catalog.DigitalSignatureAndEncryptionKeysCertificates AS CertificatesOverridable
		LEFT JOIN Catalog.DigitalSignatureAndEncryptionApplications AS ProgramsOverridable
		ON CertificatesOverridable.Application = ProgramsOverridable.Ref
		AND &amp;OptionalConnection
WHERE
	(&amp;CertificateUser = UNDEFINED
	OR CertificatesOverridable.User = &amp;CertificateUser
	OR CertificatesOverridable.Ref IN
		(SELECT DISTINCT
			ElectronicSignatureAndEncryptionKeyCertificatesUsersOverridable.Ref AS Ref
		FROM
			Catalog.DigitalSignatureAndEncryptionKeysCertificates.Users AS
				ElectronicSignatureAndEncryptionKeyCertificatesUsersOverridable
		WHERE
			ElectronicSignatureAndEncryptionKeyCertificatesUsersOverridable.User = &amp;CertificateUser))</queryText>
      <mainTable>Catalog.DigitalSignatureAndEncryptionKeysCertificates</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
  </attributes>
