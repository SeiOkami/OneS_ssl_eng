		</template>
		<parameter xmlns:dcsat="http://v8.1c.ru/8.1/data-composition-system/area-template" xsi:type="dcsat:ExpressionAreaTemplateParameter">
			<dcsat:name>MeasurementPeriod</dcsat:name>
			<dcsat:expression>Case
    When &amp;MeasurementsPeriodicity=1 Then Format(MeasurementPeriod, "DF= 'DD. MM. yyyy"" g. ""HH:mm'")+ "-" + Format(DateAdd(MeasurementPeriod,"Minute",59), "DF='HH:mm'")
    When &amp;MeasurementsPeriodicity=2 Then Format(MeasurementPeriod, "DF='DD. MM. yyyy'") + "Ð³."
    When &amp;MeasurementsPeriodicity=3 Then
        Case
            When BeginOfPeriod(MeasurementPeriod, "Week") &lt; &amp;BeginOfPeriod Then Format(&amp;BeginOfPeriod, "DF= 'DD. MM. yyyy""g""'") + " - " + Format(EndOfPeriod(MeasurementPeriod,"Week"), "DF= 'DD. MM. yyyy""g ""'")
            When EndOfPeriod(MeasurementPeriod, "Week") &gt; &amp;EndOfPeriod Then Format(BeginOfPeriod(MeasurementPeriod, "Week"), "DF= 'DD. MM. yyyy""g""'")+ " - " + Format(&amp;EndOfPeriod, "DF= 'DD. MM. yyyy""g ""'")
            Else Format(BeginOfPeriod(MeasurementPeriod, "Week"), "DF= 'DD. MM. yyyy""g""'")+ " - " + Format(EndOfPeriod(MeasurementPeriod,"Week"), "DF= 'DD. MM. yyyy""g ""'")
        End
    When &amp;MeasurementsPeriodicity=4 Then Format(&amp;BeginOfPeriod, "DF='DD. MM. yyyy'") + " - " + Format(&amp;EndOfPeriod, "DF='DD. MM. yyyy'")
End</dcsat:expression>
