		<parameter xmlns:dcsat="http://v8.1c.ru/8.1/data-composition-system/area-template" xsi:type="dcsat:ExpressionAreaTemplateParameter">
			<dcsat:name>APDEX</dcsat:name>
			<dcsat:expression>Case
    When
        APDEXPrevious IS NULL
    Then
        Format(APDEX, "BDC=3; CHN=0.000")
    Else
        Case
            When
                APDEX - APDEXPrevious &gt; 0
            Then
                Format(APDEX, "BDC=3; CHN=0.000") + "(+" + Format(APDEX - APDEXPrevious,"BDC=3; CHN=0.000") + ")"
            When
                APDEX - APDEXPrevious &lt; 0
            Then
                Format(APDEX, "BDC=3; CHN=0.000") + "(-" + Format(APDEXPrevious - APDEX,"BDC=3; CHN=0.000") + ")"
            Else
                Format(APDEX, "BDC=3; CHN=0.000")
        End
End</dcsat:expression>
