		<dataSource>DataSource1</dataSource>
		<query>SELECT
	CASE &amp;MeasurementsPeriodicity
		WHEN 1
			THEN BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, Measurements.MeasurementStartDate / 1000 - 63555667200 + &amp;TimeZoneAdjustment), HOUR)
		WHEN 2
			THEN BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, Measurements.MeasurementStartDate / 1000 - 63555667200 + &amp;TimeZoneAdjustment), DAY)
		WHEN 3
			THEN CASE
					WHEN Measurements.MeasurementStartDate &lt; &amp;PeriodStartNumber
						THEN DATEADD(DATETIME(2015, 1, 1), SECOND, &amp;PeriodStartNumber / 1000 - 63555667200 + &amp;TimeZoneAdjustment)
					ELSE BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, Measurements.MeasurementStartDate / 1000 - 63555667200 + &amp;TimeZoneAdjustment), WEEK)
				END
		WHEN 4
			THEN &amp;PeriodNo
	END AS MeasurementPeriod,
	KeyOperations.Priority AS Priority,
	KeyOperations.KeyOperation AS KeyOperation,
	COUNT(*) AS MeasurementsCount,
	SUM(Measurements.RunTime) AS MeasurementsSum,
	1 AS MeasurementID,
	(SUM(Measurements.RunTime * Measurements.RunTime) - 2 * AVG(Measurements.RunTime) * SUM(Measurements.RunTime) + COUNT(*) * (AVG(Measurements.RunTime) * AVG(Measurements.RunTime))) / COUNT(*) AS PopulationVariance,
	SUM(CASE
			WHEN Measurements.RunTime &lt;= KeyOperations.ResponseTimeThreshold
				THEN 1
			ELSE 0
		END) AS N_T,
	SUM(CASE
			WHEN Measurements.RunTime &gt; KeyOperations.ResponseTimeThreshold
					AND Measurements.RunTime &lt;= 4 * KeyOperations.ResponseTimeThreshold
				THEN 1
			ELSE 0
		END) AS N_T_4T,
	SUM(CASE
			WHEN Measurements.RunTime &gt; 4 * KeyOperations.ResponseTimeThreshold
				THEN 1
			ELSE 0
		END) AS N_4T,
	KeyOperations.ResponseTimeThreshold AS ResponseTimeThreshold,
	MIN(Measurements.RunTime) AS MinValue,
	MAX(Measurements.RunTime) AS MaxValue,
	Measurements.User,
	Measurements.CompletedWithError
INTO MeasurementsCurrent
FROM
	InformationRegister.TimeMeasurements AS Measurements
		INNER JOIN Catalog.KeyOperationProfiles.ProfileKeyOperations AS KeyOperations
		ON (KeyOperations.Ref = &amp;Profile)
			AND Measurements.KeyOperation = KeyOperations.KeyOperation
WHERE
	Measurements.MeasurementStartDate BETWEEN &amp;PeriodStartNumber AND &amp;PeriodEndNumber
{WHERE
	(Measurements.RunTime &gt;= &amp;MeasurementLowerBoundaryCoefficient * KeyOperations.ResponseTimeThreshold),
	(Measurements.RunTime &lt;= &amp;MeasurementUpperBoundaryCoefficient * KeyOperations.ResponseTimeThreshold),
	Measurements.User,
	Measurements.CompletedWithError}

GROUP BY
	CASE &amp;MeasurementsPeriodicity
		WHEN 1
			THEN BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, Measurements.MeasurementStartDate / 1000 - 63555667200 + &amp;TimeZoneAdjustment), HOUR)
		WHEN 2
			THEN BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, Measurements.MeasurementStartDate / 1000 - 63555667200 + &amp;TimeZoneAdjustment), DAY)
		WHEN 3
			THEN CASE
					WHEN Measurements.MeasurementStartDate &lt; &amp;PeriodStartNumber
						THEN DATEADD(DATETIME(2015, 1, 1), SECOND, &amp;PeriodStartNumber / 1000 - 63555667200 + &amp;TimeZoneAdjustment)
					ELSE BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, Measurements.MeasurementStartDate / 1000 - 63555667200 + &amp;TimeZoneAdjustment), WEEK)
				END
		WHEN 4
			THEN &amp;PeriodNo
	END,
	KeyOperations.KeyOperation,
	KeyOperations.Priority,
	KeyOperations.ResponseTimeThreshold,
	Measurements.User,
	Measurements.CompletedWithError

INDEX BY
	MeasurementPeriod,
	KeyOperation
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	CASE &amp;MeasurementsPeriodicity
		WHEN 1
			THEN BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, (Measurements.MeasurementStartDate + (&amp;PeriodStartNumber - &amp;ComparisonPeriodStartNumber)) / 1000 - 63555667200 + &amp;TimeZoneAdjustment), HOUR)
		WHEN 2
			THEN BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, (Measurements.MeasurementStartDate + (&amp;PeriodStartNumber - &amp;ComparisonPeriodStartNumber)) / 1000 - 63555667200 + &amp;TimeZoneAdjustment), DAY)
		WHEN 3
			THEN CASE
					WHEN Measurements.MeasurementStartDate - (&amp;PeriodStartNumber - &amp;ComparisonPeriodStartNumber) &lt; &amp;PeriodStartNumber
						THEN BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, &amp;PeriodStartNumber / 1000 - 63555667200 + &amp;TimeZoneAdjustment), WEEK)
					ELSE BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, (Measurements.MeasurementStartDate + (&amp;PeriodStartNumber - &amp;ComparisonPeriodStartNumber)) / 1000 - 63555667200 + &amp;TimeZoneAdjustment), WEEK)
				END
		WHEN 4
			THEN &amp;PeriodNo
	END AS MeasurementPeriod,
	KeyOperations.Priority AS Priority,
	KeyOperations.KeyOperation AS KeyOperation,
	COUNT(*) AS MeasurementsCount,
	SUM(Measurements.RunTime) AS MeasurementsSum,
	1 AS MeasurementID,
	(SUM(Measurements.RunTime * Measurements.RunTime) - 2 * AVG(Measurements.RunTime) * SUM(Measurements.RunTime) + COUNT(*) * (AVG(Measurements.RunTime) * AVG(Measurements.RunTime))) / COUNT(*) AS PopulationVariance,
	SUM(CASE
			WHEN Measurements.RunTime &lt;= KeyOperations.ResponseTimeThreshold
				THEN 1
			ELSE 0
		END) AS N_T,
	SUM(CASE
			WHEN Measurements.RunTime &gt; KeyOperations.ResponseTimeThreshold
					AND Measurements.RunTime &lt;= 4 * KeyOperations.ResponseTimeThreshold
				THEN 1
			ELSE 0
		END) AS N_T_4T,
	SUM(CASE
			WHEN Measurements.RunTime &gt; 4 * KeyOperations.ResponseTimeThreshold
				THEN 1
			ELSE 0
		END) AS N_4T,
	KeyOperations.ResponseTimeThreshold AS ResponseTimeThreshold,
	MIN(Measurements.RunTime) AS MinValue,
	MAX(Measurements.RunTime) AS MaxValue,
	Measurements.User,
	Measurements.CompletedWithError
INTO MeasurementsPrevious
FROM
	InformationRegister.TimeMeasurements AS Measurements
		INNER JOIN Catalog.KeyOperationProfiles.ProfileKeyOperations AS KeyOperations
		ON (KeyOperations.Ref = &amp;Profile)
			AND Measurements.KeyOperation = KeyOperations.KeyOperation
WHERE
	Measurements.MeasurementStartDate BETWEEN &amp;ComparisonPeriodStartNumber AND &amp;ComparisonPeriodEndNumber
{WHERE
	Measurements.User,
	Measurements.CompletedWithError}

GROUP BY
	CASE &amp;MeasurementsPeriodicity
		WHEN 1
			THEN BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, (Measurements.MeasurementStartDate + (&amp;PeriodStartNumber - &amp;ComparisonPeriodStartNumber)) / 1000 - 63555667200 + &amp;TimeZoneAdjustment), HOUR)
		WHEN 2
			THEN BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, (Measurements.MeasurementStartDate + (&amp;PeriodStartNumber - &amp;ComparisonPeriodStartNumber)) / 1000 - 63555667200 + &amp;TimeZoneAdjustment), DAY)
		WHEN 3
			THEN CASE
					WHEN Measurements.MeasurementStartDate - (&amp;PeriodStartNumber - &amp;ComparisonPeriodStartNumber) &lt; &amp;PeriodStartNumber
						THEN BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, &amp;PeriodStartNumber / 1000 - 63555667200 + &amp;TimeZoneAdjustment), WEEK)
					ELSE BEGINOFPERIOD(DATEADD(DATETIME(2015, 1, 1), SECOND, (Measurements.MeasurementStartDate + (&amp;PeriodStartNumber - &amp;ComparisonPeriodStartNumber)) / 1000 - 63555667200 + &amp;TimeZoneAdjustment), WEEK)
				END
		WHEN 4
			THEN &amp;PeriodNo
	END,
	KeyOperations.KeyOperation,
	KeyOperations.Priority,
	KeyOperations.ResponseTimeThreshold,
	Measurements.User,
	Measurements.CompletedWithError

INDEX BY
	MeasurementPeriod,
	KeyOperation
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	ISNULL(MeasurementsCurrent.Priority,MeasurementsPrevious.Priority) AS Priority,
	ISNULL(MeasurementsCurrent.MeasurementPeriod,MeasurementsPrevious.MeasurementPeriod) AS MeasurementPeriod,
	ISNULL(MeasurementsCurrent.KeyOperation,MeasurementsPrevious.KeyOperation) AS KeyOperation,
	MeasurementsCurrent.MeasurementsCount AS MeasurementsCount,
	MeasurementsPrevious.MeasurementsCount AS MeasurementsCountPrevious,
	MeasurementsCurrent.MeasurementsSum AS MeasurementsSum,
	MeasurementsPrevious.MeasurementsSum AS MeasurementsSumPrevious,
	MeasurementsCurrent.MeasurementID AS MeasurementID,
	MeasurementsPrevious.MeasurementID AS MeasurementIDPrevious,
	MeasurementsCurrent.PopulationVariance AS PopulationVariance,
	MeasurementsPrevious.PopulationVariance AS PopulationVariancePrevious,
	MeasurementsCurrent.N_T AS N_T,
	MeasurementsPrevious.N_T AS NTPrevious,
	MeasurementsCurrent.N_T_4T AS N_T_4T,
	MeasurementsPrevious.N_T_4T AS NT4TNext,
	MeasurementsCurrent.N_4T AS N_4T,
	MeasurementsPrevious.N_4T AS N4TPrevious,
	ISNULL(MeasurementsCurrent.ResponseTimeThreshold, MeasurementsPrevious.ResponseTimeThreshold) AS ResponseTimeThreshold,
	MeasurementsCurrent.MinValue AS MinValue,
	MeasurementsPrevious.MinValue AS MinValuePrevious,
	MeasurementsCurrent.MaxValue AS MaxValue,
	MeasurementsPrevious.MaxValue AS MaxValuePrevious,
	ISNULL(MeasurementsCurrent.User, MeasurementsPrevious.User) AS User,
	ISNULL(MeasurementsCurrent.CompletedWithError, MeasurementsPrevious.CompletedWithError) AS CompletedWithError
FROM
	MeasurementsCurrent AS MeasurementsCurrent
		{LEFT JOIN MeasurementsPrevious AS MeasurementsPrevious
		ON (MeasurementsPrevious.MeasurementPeriod = MeasurementsCurrent.MeasurementPeriod)
			AND (MeasurementsPrevious.KeyOperation = MeasurementsCurrent.KeyOperation)
			AND (MeasurementsPrevious.User = MeasurementsCurrent.User)
			AND (MeasurementsPrevious.CompletedWithError = MeasurementsCurrent.CompletedWithError)}</query>
	</dataSet>
	<calculatedField>
		<dataPath>APDEX</dataPath>
		<expression>Cast((Sum(N_T) + Sum(N_T_4T/2))/Sum(MeasurementsCount), "Number(4,3)")</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>APDEX</v8:content>
			</v8:item>
		</title>
		<appearance>
			<dcscor:item xsi:type="dcsset:SettingsParameterValue">
				<dcscor:parameter>Format</dcscor:parameter>
				<dcscor:value xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>en</v8:lang>
						<v8:content>NFD=3; NZ=0.000</v8:content>
					</v8:item>
				</dcscor:value>
			</dcscor:item>
		</appearance>
	</calculatedField>
	<calculatedField>
		<dataPath>AverageMeasurementValue</dataPath>
		<expression>Cast(Sum(MeasurementsSum)/Sum(MeasurementsCount), "Number(15,3)")</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Average</v8:content>
			</v8:item>
		</title>
		<appearance>
			<dcscor:item xsi:type="dcsset:SettingsParameterValue">
				<dcscor:parameter>Format</dcscor:parameter>
				<dcscor:value xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>en</v8:lang>
						<v8:content>NFD=3; NZ=0.000</v8:content>
					</v8:item>
				</dcscor:value>
			</dcscor:item>
		</appearance>
	</calculatedField>
	<calculatedField>
		<dataPath>StdDeviation</dataPath>
		<expression>Case When Sum(MeasurementsCount)=1 Then 0 Else Sqrt(((Sum(EvalExpressionWithGroupArray("PopulationVariance*Sum(MeasurementsCount)"))/Sum(EvalExpressionWithGroupArray("Sum(MeasurementsCount)"))+
Sum(EvalExpressionWithGroupArray("(EvalExpression(""Sum(MeasurementsSum)/Sum(MeasurementsCount)"",,,""First"", ""Last"") - (Sum(MeasurementsSum)/Sum(MeasurementsCount))) * (EvalExpression(""Sum(MeasurementsSum)/Sum(MeasurementsCount)"",,,""First"", ""Last"") - (Sum(MeasurementsSum)/Sum(MeasurementsCount))) * MeasurementsCount"))/Sum(MeasurementsCount))*Sum(MeasurementsCount))/(Sum(MeasurementsCount) - 1))
End</expression>
