	#If StrContains(&amp;CommonAccessRestrictionTemplateParameters, ";00^;") #Then ,TRUE #Else ,FALSE #EndIf
	)
  #EndIf
 #EndIf

#EndIf</condition>
	</restrictionTemplate>
