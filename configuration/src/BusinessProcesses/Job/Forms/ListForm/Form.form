    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Suspended</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	BusinessProcessTask.Ref,
	BusinessProcessTask.DeletionMark,
	BusinessProcessTask.Number,
	BusinessProcessTask.Date,
	BusinessProcessTask.Completed,
	BusinessProcessTask.HeadTask,
	BusinessProcessTask.Started,
	BusinessProcessTask.Author,
	BusinessProcessTask.Importance,
	BusinessProcessTask.Completed2,
	BusinessProcessTask.CompletedOn,
	BusinessProcessTask.Performer,
	BusinessProcessTask.Description,
	BusinessProcessTask.OnValidation,
	BusinessProcessTask.IterationNumber,
	BusinessProcessTask.Content,
	BusinessProcessTask.Accepted,
	BusinessProcessTask.SubjectOf,
	BusinessProcessTask.Supervisor,
	BusinessProcessTask.ExecutionResult,
	BusinessProcessTask.TaskDueDate,
	BusinessProcessTask.VerificationDueDate,
	CASE
		WHEN BusinessProcessTask.DeletionMark
			THEN 6
		WHEN BusinessProcessTask.Completed = TRUE
			THEN 5
		WHEN BusinessProcessTask.State = VALUE(Enum.BusinessProcessStates.Suspended)
			THEN 4
		WHEN BusinessProcessTask.Started = TRUE
			THEN 3
		WHEN ProcessesToStart.State = VALUE(Enum.ProcessesStatesForStart.ReadyToStart)
				AND ProcessesToStart.DeferredStartDate &lt;> DATETIME(1, 1, 1)
			THEN 1
		WHEN ProcessesToStart.State = VALUE(Enum.ProcessesStatesForStart.StartCanceled)
			THEN 2
		ELSE 0
	END AS RowPicture,
	CASE
		WHEN BusinessProcessTask.Importance = VALUE(Enum.TaskImportanceOptions.Low)
			THEN 0
		WHEN BusinessProcessTask.Importance = VALUE(Enum.TaskImportanceOptions.High)
			THEN 2
		ELSE 1
	END AS ImportancePicture,
	CASE
		WHEN BusinessProcessTask.State = VALUE(Enum.BusinessProcessStates.Suspended)
			THEN TRUE
		ELSE FALSE
	END AS Suspended,
	BusinessProcessTask.SourceTask,
	CASE
		WHEN ProcessesToStart.State IS NULL 
			THEN FALSE
		ELSE TRUE
	END AS Defer
FROM
	BusinessProcess.Job AS BusinessProcessTask
		LEFT JOIN InformationRegister.ProcessesToStart AS ProcessesToStart
		ON BusinessProcessTask.Ref = ProcessesToStart.Owner</queryText>
      <mainTable>BusinessProcess.Job</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
