    <name>List</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.ExportUsageFlag</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Flagusage</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.ImportUsageFlag</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.UseScheduledJob</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
    DataExchangeScenariosExchangeSettings.Ref AS Ref,
    DataExchangeScenariosExchangeSettings.Ref.UseScheduledJob AS UseScheduledJob,
    DataExchangeScenariosExchangeSettings.Ref.Description AS Description,
    MAX(CASE
            WHEN DataExchangeScenariosExchangeSettings.InfobaseNode = &amp;InfobaseNode
                THEN TRUE
            ELSE FALSE
        END) AS Flagusage,
    MAX(CASE
            WHEN DataExchangeScenariosExchangeSettings.InfobaseNode = &amp;InfobaseNode
                    AND DataExchangeScenariosExchangeSettings.CurrentAction = VALUE(Enum.ActionsOnExchange.DataImport)
                THEN TRUE
            ELSE FALSE
        END) AS ImportUsageFlag,
    MAX(CASE
            WHEN DataExchangeScenariosExchangeSettings.InfobaseNode = &amp;InfobaseNode
                    AND DataExchangeScenariosExchangeSettings.CurrentAction = VALUE(Enum.ActionsOnExchange.DataExport)
                THEN TRUE
            ELSE FALSE
        END) AS ExportUsageFlag
INTO NestedQuery
FROM
    Catalog.DataExchangeScenarios.ExchangeSettings AS DataExchangeScenariosExchangeSettings
WHERE
    NOT DataExchangeScenariosExchangeSettings.Ref.DeletionMark

GROUP BY
    DataExchangeScenariosExchangeSettings.Ref,
    DataExchangeScenariosExchangeSettings.Ref.UseScheduledJob,
    DataExchangeScenariosExchangeSettings.Ref.Description
;
SELECT
	DataExchangeScenarios.Ref,
	DataExchangeScenarios.UseScheduledJob,
	DataExchangeScenarios.Description,
	ISNULL(NestedQuery.Flagusage, FALSE) AS Flagusage,
	ISNULL(NestedQuery.ImportUsageFlag, FALSE) AS ImportUsageFlag,
	ISNULL(NestedQuery.ExportUsageFlag, FALSE) AS ExportUsageFlag,
	DataExchangeScenarios.Comment
FROM
	Catalog.DataExchangeScenarios AS DataExchangeScenarios
		LEFT JOIN NestedQuery AS NestedQuery
		ON NestedQuery.Ref = DataExchangeScenarios.Ref
WHERE
	NOT DataExchangeScenarios.DeletionMark
</queryText>
      <mainTable>Catalog.DataExchangeScenarios</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
