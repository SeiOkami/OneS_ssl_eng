  <attributes>
    <name>List</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.AcceptedForExecution</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.BusinessProcess</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.SubjectOf</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	TasksByExecutive.Ref,
	TasksByExecutive.DeletionMark,
	TasksByExecutive.Number,
	TasksByExecutive.Date,
	TasksByExecutive.BusinessProcess,
	TasksByExecutive.RoutePoint,
	TasksByExecutive.Description,
	TasksByExecutive.Executed,
	TasksByExecutive.TaskDueDate,
	CASE
		WHEN TasksByExecutive.StartDate = &amp;BlankDate
			THEN &amp;BegOfDay
		ELSE TasksByExecutive.StartDate
	END AS StartDate,
	TasksByExecutive.CompletionDate,
	TasksByExecutive.Importance,
	TasksByExecutive.LongDesc,
	TasksByExecutive.PerformerRole,
	TasksByExecutive.Performer,
	TasksByExecutive.Presentation,
	TasksByExecutive.SubjectString,
	TasksByExecutive.Author,
	TasksByExecutive.AcceptedForExecution,
	CASE
		WHEN TasksByExecutive.Importance = VALUE(Enum.TaskImportanceOptions.Low)
			THEN 0
		WHEN TasksByExecutive.Importance = VALUE(Enum.TaskImportanceOptions.High)
			THEN 2
		ELSE 1
	END AS ImportancePicture,
	TasksByExecutive.SubjectOf,
	CASE
		WHEN TasksByExecutive.TaskDueDate &lt;= &amp;CurrentDate
			THEN &amp;Overdue
		WHEN TasksByExecutive.TaskDueDate > &amp;CurrentDate AND TasksByExecutive.TaskDueDate &lt;= &amp;EndOfDay
			THEN &amp;Today
		WHEN TasksByExecutive.TaskDueDate > &amp;EndOfDay AND TasksByExecutive.TaskDueDate &lt;= &amp;EndOfWeek
			THEN &amp;ThisWeek
		WHEN TasksByExecutive.TaskDueDate > &amp;EndOfWeek AND TasksByExecutive.TaskDueDate &lt;= &amp;EndOfNextWeek
			THEN &amp;NextWeek
		ELSE &amp;Later
	END AS GroupDueDate
FROM
	Task.PerformerTask.TasksByExecutive AS TasksByExecutive
WHERE
	TasksByExecutive.BusinessProcessState = VALUE(Enum.BusinessProcessStates.Running)</queryText>
      <mainTable>Task.PerformerTask.TasksByExecutive</mainTable>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
