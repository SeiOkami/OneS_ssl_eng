		<item xsi:type="DataSetQuery">
			<name>FieldDefinition</name>
			<field xsi:type="DataSetFieldField">
				<dataPath>RegistrationObject</dataPath>
				<field>RegistrationObject</field>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>RegistrationObjectType</dataPath>
				<field>RegistrationObjectType</field>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>RegistrationReason</dataPath>
				<field>RegistrationReason</field>
			</field>
			<dataSource>CommonDataSource</dataSource>
			<query>SELECT
	NULL 						AS RegistrationObject,
	NULL 						AS RegistrationObjectType,
	&amp;RegistrationReasonByRef AS RegistrationReason
{WHERE
	(&amp;RegistrationReasonByRef) AS RegistrationReason}
WHERE FALSE</query>
		</item>
