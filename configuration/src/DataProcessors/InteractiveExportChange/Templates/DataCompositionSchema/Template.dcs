	<totalField>
		<dataPath>RegistrationAutomatically</dataPath>
		<expression>Sum(RegistrationAutomatically)</expression>
		<group>FullMetadataName</group>
		<group>Overall</group>
	</totalField>
	<totalField>
		<dataPath>RegistrationAdditionally</dataPath>
		<expression>Sum(RegistrationAdditionally)</expression>
		<group>FullMetadataName</group>
		<group>Overall</group>
	</totalField>
	<totalField>
		<dataPath>RegistrationByRef</dataPath>
		<expression>Sum(RegistrationByRef)</expression>
		<group>FullMetadataName</group>
		<group>Overall</group>
	</totalField>
	<totalField>
		<dataPath>CommonCount</dataPath>
		<expression>Sum(CommonCount)</expression>
		<group>FullMetadataName</group>
		<group>Overall</group>
	</totalField>
	<totalField>
		<dataPath>CountByGeneralRules</dataPath>
		<expression>Sum(RegistrationAutomatically)
</expression>
		<group>FullMetadataName</group>
		<group>Overall</group>
	</totalField>
	<totalField>
		<dataPath>ToExportCount</dataPath>
		<expression>Case
  When Not CanExportObject Then 0
  When Sum(RegistrationAutomatically)&gt;0  Then 1
  When Sum(RegistrationAdditionally)&gt;0  Then 1
  Else 0
End</expression>
	</totalField>
	<totalField>
		<dataPath>ToExportCount</dataPath>
		<expression>Count(Distinct 
Case
  When Not CanExportObject Then NULL
  When RegistrationAutomatically&gt;0  Then RegistrationObject
  When RegistrationAdditionally&gt;0  Then RegistrationObject
  Else NULL
End
)</expression>
		<group>FullMetadataName</group>
		<group>Overall</group>
	</totalField>
	<totalField>
		<dataPath>NoExport</dataPath>
		<expression>Sum(CommonCount)
-
Count(Distinct 
Case
  When Not CanExportObject Then NULL
  When RegistrationAutomatically&gt;0  Then RegistrationObject
  When RegistrationAdditionally&gt;0  Then RegistrationObject
  Else NULL
End
)</expression>
		<group>FullMetadataName</group>
		<group>Overall</group>
	</totalField>
