	</calculatedField>
	<calculatedField>
		<dataPath>SimplifiedMode</dataPath>
		<expression>&amp;SimplifiedMode</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>en</v8:lang>
