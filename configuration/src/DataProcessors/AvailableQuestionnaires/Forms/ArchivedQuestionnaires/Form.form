    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>CompletedSurveys.SurveyDescription</segments>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	Questionnaire.Ref AS Ref,
	ISNULL(PollPurpose.Description, &quot;&quot;) AS SurveyDescription,
	Questionnaire.EditDate AS FillingDate,
	0 AS PictureCode
FROM
	Document.Questionnaire AS Questionnaire
		LEFT JOIN Document.PollPurpose AS PollPurpose
		ON Questionnaire.Survey = PollPurpose.Ref
WHERE
	Questionnaire.Posted
	AND Questionnaire.Respondent = &amp;Respondent
	AND PollPurpose.ShowInQuestionnaireArchive</queryText>
      <mainTable>Document.Questionnaire</mainTable>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
