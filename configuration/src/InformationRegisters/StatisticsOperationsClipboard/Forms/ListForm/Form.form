    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	DATEADD(
		DATETIME(2015,1,1),
		SECOND,
		(CAST(StatisticsOperationsClipboard.RecordDate/1000 - 0.5 AS NUMBER(15,0)) * 1000)/1000  - 63555667200
	) AS Period,
	StatisticsOperationsClipboard.RecordDate,
	StatisticsOperationsClipboard.DeletionID,
	StatisticsOperationsClipboard.RecordID,
	StatisticsOperations.Description AS StatisticsOperationDescription,
	StatisticsComments.Description AS StatisticsCommentDescription,
	StatisticsAreas.Description AS StatisticsAreaDescription,
	StatisticsOperationsClipboard.Value,
	StatisticsOperationsClipboard.StatisticsOperation,
	StatisticsOperationsClipboard.StatisticsComment,
	StatisticsOperationsClipboard.StatisticsArea
FROM
	InformationRegister.StatisticsOperationsClipboard AS StatisticsOperationsClipboard
LEFT JOIN
	InformationRegister.StatisticsOperations AS StatisticsOperations
ON
	StatisticsOperationsClipboard.StatisticsOperation = StatisticsOperations.OperationID
LEFT JOIN
	InformationRegister.StatisticsComments AS StatisticsComments
ON
	StatisticsOperationsClipboard.StatisticsComment = StatisticsComments.CommentID
LEFT JOIN
	InformationRegister.StatisticsAreas AS StatisticsAreas
ON
	StatisticsOperationsClipboard.StatisticsArea = StatisticsAreas.AreaID</queryText>
