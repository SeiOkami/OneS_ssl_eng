    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	FilesInVolume.Ref,
	FilesVersions.PictureIndex,
	FilesVersions.PathToFile,
	FilesVersions.Author,
	FilesVersions.Size
